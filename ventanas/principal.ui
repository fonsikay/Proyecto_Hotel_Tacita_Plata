{
	color: rgb(225, 225, 225);
	font: 25 12pt &quot;Bahnschrift Light&quot;;
}

QLineEdit#txtUsuario, #txtPassword
{
	border: 1px solid rgb(198, 160, 30);
	border-radius: 10px;
	padding: 15px;
	margin-top:10px;
	margin-right:10px;
	background-color: rgb(225, 225, 225);
	color: rgb(77, 77, 77);
	font: 25 10pt &quot;Bahnschrift Light&quot;;
}

QLineEdit#txtUsuario:hover, #txtPassword:hover
{
	border: 2px solid rgb(0, 66, 124);
}

QLineEdit#txtUsuario:focus, #txtPassword:focus
{
	border: 2px solid rgb(206, 206, 206);
	color: rgb(52, 52, 52);
}

QCheckBox#chkSesion
{
	color: rgb(225, 225, 225);
	font: 25 10pt &quot;Bahnschrift Light&quot;;
}

QCheckBox#chkSesion::indicator
{
	border: 1px solid rgb(198, 160, 30);
	width: 20px;
	height: 20px;
	border-radius: 5px;
	background-color: rgb(225, 225, 225);
	margin-right: 3px;
	padding: 2px;
}

QCheckBox#chkSesion::indicator:hover
{
	border: 1px solid rgb(0, 66, 124);
}

QCheckBox#chkSesion::indicator:checked
{
	background: 2px solid rgb(198, 160, 30);
	background-image: url(:/iconos/iconos/cil-check.png);
}

QPushButton#btnLogin
{
	border: 1px solid rgb(198, 160, 30);
	border-radius: 10px;
	padding: 15px;
	margin-top:10px;
	margin-right:10px;
	background-color: qlineargradient(spread:pad, x1:0.528, y1:0.858, x2:0.517409, y2:0.159, stop:0 rgba(23, 55, 87, 255), stop:1 rgba(42, 100, 165, 255));
	color: rgb(225, 225, 225);
	font: 25 10pt &quot;Bahnschrift Light&quot;;
}

/* Color del botón Minimizar cuando se posiciona el cursor */
QPushButton#btnLogin:hover
{
  	background-color:qlineargradient(spread:pad, x1:0.528409, y1:0.733, 					x2:0.5, y2:0.17, stop:0 #b28d0b, stop:1 #f5cc3d);
	font-weight: bold;
}

/* Color del botón Minimizar cuando se pulsa el botón */
QPushButton#btnLogin:pressed
{
	font-weight: bold;
	background-color: #b28d0b;
}


